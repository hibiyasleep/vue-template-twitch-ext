{
  "name": "{{ name }}",
  "version": "0.0.1",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "engines": {
    "node": ">=12.16.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack-cli",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development webpack-dev-server --mode=development --config=webpack.config.js --progress",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "axios": "^0.19",
    "vue": "2.x",
    "vue-router": "3.x",
    "vuex": "3.x"
  },
  "devDependencies": {
    "@babel/core": "7.x",
    "@babel/plugin-proposal-object-rest-spread": "7.x",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@vue/component-compiler-utils": "3.1.2",
    "babel-loader": "8.x",
    "copy-webpack-plugin": "latest",
    "css-loader": "^3.2.0",
    "file-loader": "^3.0.1",
    "node-sass": "latest",
    {{#if_eq lintConfig "prettier"}}
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^2.0.5",
    {{/if_eq}}
    "html-webpack-plugin": "latest",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.3",
    "script-ext-html-webpack-plugin": "latest",
    "script-loader": "^0.7.2",
    "uglifyjs-webpack-plugin": "latest",
    "vue-hot-reload-api": "2.x",
    "vue-loader": "15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.x",
    {{#if_eq i18n true}}
    "vuex-i18n": "^1.13.1",
    {{/if_eq}}
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest",
    "zip-webpack-plugin": "latest"
  }
}
